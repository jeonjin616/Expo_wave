<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- SQL문 정의할 때 VO표현을 간단하게 하고 싶은 경우. 별명을 지어서 사용 -->
	<typeAliases>
		<typeAlias type="com.multi.wave.member.MemberVO" alias="memberVO"></typeAlias>
		<typeAlias type="com.multi.wave.showdetail.ShowDetailVO" alias="ShowDetailVO"></typeAlias>
		<typeAlias type="com.multi.wave.festivaldetail.FestivalDetailVO" alias="FestivalDetailVO"></typeAlias>
      	<typeAlias type="com.multi.wave.md.MDPostDTO" alias="mdPostDTO"></typeAlias>
      	<typeAlias type="com.multi.wave.md.MDImgDTO" alias="mdImgDTO"></typeAlias>
      	<typeAlias type="com.multi.wave.md.MDCommentDTO" alias="mdCommentDTO"></typeAlias>
      	<typeAlias type="com.multi.wave.festival.FestivalVO" alias="festivalVO"></typeAlias>
		<typeAlias type="com.multi.wave.show.ShowVO" alias="showVO"></typeAlias>
		<typeAlias type="com.multi.wave.notice.BoardVO" alias="BoardVO" />
		<typeAlias type="com.multi.wave.notice.FaqVO" alias="FaqVO" />
		<typeAlias type="com.multi.wave.notice.InquiryVO" alias="InquiryVO" />
		<typeAlias type="com.multi.wave.notice.ReplyVO" alias="ReplyVO" />
		
		</typeAliases>
	<mappers>
		<!-- SQL문 정의하는 파일들의 목록을 지정. 테이블당 한개 사용 -->
		<mapper resource="mapper/memberMapper.xml" />
		<mapper resource="mapper/ShowDetailMapper.xml" />
		<mapper resource="mapper/FestivalDetailMapper.xml" />
		<mapper resource="mapper/MDPostMapper.xml" />
      	<mapper resource="mapper/MDImgMapper.xml"/>
      	<mapper resource="mapper/MDCommentMapper.xml"/>
      	<mapper resource="mapper/festivalMapper.xml" />
		<mapper resource="mapper/showMapper.xml" />
		<mapper resource="mapper/boardMapper.xml" />
		<mapper resource="mapper/faqMapper.xml" />
		<mapper resource="mapper/inquiryMapper.xml" />
		<mapper resource="mapper/replyMapper.xml" />
		
	</mappers>
</configuration>
